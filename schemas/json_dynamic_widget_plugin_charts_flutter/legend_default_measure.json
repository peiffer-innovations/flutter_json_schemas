{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_dynamic_widget_plugin_charts_flutter/legend_default_measure.json",
  "$comment": "https://pub.dev/documentation/charts_common/latest/common/LegendDefaultMeasure.html",
  "type": "string",
  "title": "LegendDefaultMeasure",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "average",
        "firstValue",
        "lastValue",
        "none",
        "sum"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
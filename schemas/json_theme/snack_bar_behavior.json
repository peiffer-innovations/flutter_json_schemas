{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/snack_bar_behavior.json",
  "$comment": "https://api.flutter.dev/flutter/material/SnackBarBehavior-class.html",
  "title": "SnackBarBehavior",
  "type": "string",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "fixed",
        "floating"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
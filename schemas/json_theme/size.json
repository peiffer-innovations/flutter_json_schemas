{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/size.json",
  "$comment": "https://api.flutter.dev/flutter/dart-ui/Size-class.html",
  "title": "Size",
  "additionalProperties": false,
  "required": [
    "height",
    "width"
  ],
  "properties": {
    "height": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "width": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    }
  }
}
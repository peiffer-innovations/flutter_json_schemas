{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/text_input_action.json",
  "type": "string",
  "title": "TextInputAction",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "continueAction",
        "done",
        "emergencyCall",
        "go",
        "join",
        "newline",
        "next",
        "none",
        "previous",
        "route",
        "search",
        "send",
        "unspecified"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
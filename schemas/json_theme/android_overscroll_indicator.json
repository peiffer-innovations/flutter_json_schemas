{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/android_overscroll_indicator.json",
  "type": "string",
  "title": "AndroidOverscrollIndicator",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "glow",
        "stretch"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/icon_theme_data.json",
  "$comment": "https://api.flutter.dev/flutter/widgets/IconThemeData-class.html",
  "title": "IconThemeData",
  "type": "object",
  "properties": {
    "color": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/color.json"
    },
    "opacity": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "shadows": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "array",
          "items": {
            "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/shadow.json"
          }
        }
      ]
    },
    "size": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    }
  }
}
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/font_weight.json",
  "$comment": "https://api.flutter.dev/flutter/dart-ui/FontWeight-class.html",
  "type": "string",
  "title": "FontWeight",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "bold",
        "normal",
        "w100",
        "w200",
        "w300",
        "w400",
        "w500",
        "w600",
        "w700",
        "w800",
        "w900"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
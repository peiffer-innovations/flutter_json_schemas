{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/stroke_cap.json",
  "type": "string",
  "title": "StrokeCap",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "butt",
        "round",
        "square"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
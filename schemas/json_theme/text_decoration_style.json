{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/text_decoration_style.json",
  "type": "string",
  "title": "TextDecorationStyle",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "dashed",
        "dotted",
        "double",
        "solid",
        "wavy"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
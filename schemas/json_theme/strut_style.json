{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/strut_style.json",
  "$comment": "https://api.flutter.dev/flutter/painting/StrutStyle-class.html",
  "title": "StrutStyle",
  "type": "object",
  "properties": {
    "fontFamily": {
      "type": "string"
    },
    "fontFamilyFallback": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "fontSize": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "fontStyle": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/font_style.json"
    },
    "fontWeight": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/font_weight.json"
    },
    "forceStrutHeight": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        },
        {
          "type": "boolean"
        }
      ]
    },
    "height": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "leading": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "package": {
      "type": "string"
    }
  }
}
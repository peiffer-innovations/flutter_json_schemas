{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_dynamic_widget/animated_switcher.json",
  "$comment": "https://api.flutter.dev/flutter/widgets/AnimatedSwitcher-class.html",
  "type": "object",
  "title": "AnimatedSwitcher",
  "additionalProperties": false,
  "required": [
    "child",
    "duration"
  ],
  "properties": {
    "child": {
      "type": "string"
    },
    "duration": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "layoutBuilder": {
      "type": "string"
    },
    "reverseDuration": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "switchInCurve": {
      "type": "string"
    },
    "switchOutCurve": {
      "type": "string"
    },
    "transitionBuilder": {
      "type": "string"
    }
  }
}
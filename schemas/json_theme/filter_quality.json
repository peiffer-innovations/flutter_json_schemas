{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/filter_quality.json",
  "type": "string",
  "title": "FilterQuality",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "high",
        "low",
        "medium",
        "none"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
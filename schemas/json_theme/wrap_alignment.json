{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/wrap_alignment.json",
  "type": "string",
  "title": "WrapAlignment",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "center",
        "end",
        "spaceAround",
        "spaceBetween",
        "spaceEvenly",
        "start"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
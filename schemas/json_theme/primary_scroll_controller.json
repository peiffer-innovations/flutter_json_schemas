{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_dynamic_widget/primary_scroll_controller.json",
  "$comment": "https://api.flutter.dev/flutter/widgets/PrimaryScrollController-class.html",
  "type": "object",
  "title": "PrimaryScrollController",
  "additionalProperties": false,
  "properties": {
    "automaticallyInheritForPlatforms": {
      "type": "array",
      "items": {
        "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/target_platform.json"
      }
    },
    "controller": {
      "type": "string"
    },
    "debugLabel": {
      "type": "string"
    },
    "initialScrollOffset": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "keepScrollOffset": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        },
        {
          "type": "boolean"
        }
      ]
    },
    "scrollDirection": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/axis.json"
    }
  }
}
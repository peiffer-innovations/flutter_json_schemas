{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/tab_alignment.json",
  "type": "string",
  "title": "TabAlignment",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "center",
        "fill",
        "start",
        "startOffset"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    },
    {
      "type": "string",
      "pattern": "\\$\\{.*\\}$"
    }
  ]
}
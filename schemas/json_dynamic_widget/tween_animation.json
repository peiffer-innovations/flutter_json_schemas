{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_dynamic_widget/tween_animation.json",
  "$comment": "https://api.flutter.dev/flutter/animation/Tween-class.html",
  "type": "object",
  "title": "TweenAnimation",
  "additionalProperties": false,
  "required": [
    "builder",
    "duration",
    "tween"
  ],
  "properties": {
    "builder": {
      "type": "string"
    },
    "curve": {
      "type": "string"
    },
    "duration": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "onEnd": {
      "type": "string"
    },
    "tween": {
      "type": "string"
    }
  }
}
{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_dynamic_widget/animated_theme.json",
  "$comment": "https://api.flutter.dev/flutter/material/AnimatedTheme-class.html",
  "type": "object",
  "title": "AnimatedThemeBuilder",
  "additionalProperties": false,
  "required": [
    "data"
  ],
  "properties": {
    "curve": {
      "type": "string"
    },
    "data": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/theme_data.json"
        }
      ]
    },
    "duration": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "onEnd": {
      "type": "string"
    }
  }
}
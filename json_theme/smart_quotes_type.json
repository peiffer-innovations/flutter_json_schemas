{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/smart_quotes_type.json",
  "$comment": "https://api.flutter.dev/flutter/services/SmartQuotesType-class.html",
  "title": "SmartQuotesType",
  "type": "string",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "disabled",
        "enabled"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    }
  ]
}